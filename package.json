{
  "name": "vue-test",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "Axel Mousset <pymzorr@gmail.com>",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "postinstall": "cd cordova && mkdir www && cordova prepare",
    "browser": "static dist --port 8081",
    "electron": "electron .",
    "android": "cd cordova && cordova run android --emulator",
    "android:device": "cp -rf dist cordova/www && cd cordova && cordova run android --device",
    "dev:android": "cross-env TARGET=cordova PLATFORM=android webpack --config build/webpack.dev.conf.js --progress --hide-modules --watch",
    "dev:browser": "cross-env TARGET=browser node build/dev-server.js",
    "dev:electron": "cross-env TARGET=electron NODE_ENV=development node build/dev-server.js",
    "build:browser": "cross-env TARGET=browser node build/build.js",
    "build:electron": "cross-env TARGET=electron node build/build.js",
    "build:android": "cross-env TARGET=cordova PLATFORM=android node build/build.js"
  },
  "dependencies": {
    "electron": "^1.6.10",
    "post-compile-webpack-plugin": "^0.1.1",
    "vue": "^2.3.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cordova": "^7.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "electron-packager": "^8.7.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "node-static": "^0.7.9",
    "opn": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "ora": "^1.2.0",
    "rimraf": "^2.6.0",
    "semver": "^5.3.0",
    "shelljs": "^0.7.8",
    "url-loader": "^0.5.8",
    "vue-loader": "^12.1.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
